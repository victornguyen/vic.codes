{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/no-unused-prop-types": 1,
    "react/no-unused-state": 1,
    "react/sort-prop-types": [1, {
      "callbacksLast": true,
      "ignoreCase": true,
      "requiredFirst": false,
      "sortShapeProp": true
    }],
    "prettier/prettier": "error"
  }
}
